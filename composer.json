{
    "name": "psergiu-dev/omnipay-maib",
    "description": "Omnipay plugin for Maib ECOMM recurring payments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergiu Popa",
            "email": "popa.sergiu920@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PsergiuDev\\OmnipayMaib\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.3",
        "ext-curl": "*",
        "omnipay/common": "^3",
        "php-http/guzzle6-adapter": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5"
    }
}
